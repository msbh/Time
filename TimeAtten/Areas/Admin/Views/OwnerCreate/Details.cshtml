@model TimeAtten.Framework.Admin.OwnerCreation

@{
    ViewBag.Title = "Detail Owner";
    string Country=ViewBag.Nation;
    string Religion = ViewBag.Religion;
    TimeAtten.Services.Utilities.BreadCrumbList breadcrumbs = ViewBag.BreadCrums;
    string err = "";
    try
    {
        if(TempData["ErrorMessage"].ToString()!=""){err = "Caution ! " + TempData["ErrorMessage"].ToString();}
    }
    catch (Exception ert) { }
}
@Html.Partial("_header")
<!--close-Header-part-->
<!-- BEGIN CONTAINER -->
<div class="page-container row-fluid">
    @Html.Partial("_left", "OwnerCreate")
    <!-- BEGIN PAGE -->
    <div class="page-content">

        <!-- BEGIN PAGE CONTAINER-->
        <div class="container-fluid">
            <!-- BEGIN PAGE HEADER-->
            @{
                Html.RenderPartial("_breadcrumb", breadcrumbs);}
            <!-- END PAGE HEADER-->
            <!-- Form -->
            <div class="portlet box green">
                <div class="portlet-title">
                    <h4><i class="icon-reorder"></i>Owner Detail</h4>
                    <div class="tools">
                        <a href="javascript:;" class="collapse"></a>
                        <a href="#portlet-config" data-toggle="modal" class="config"></a>
                        <a href="javascript:;" class="reload"></a>
                        <a href="javascript:;" class="remove"></a>
                    </div>
                </div>
                <div id="err" style="color:#F80606;">
                    @err
                </div>
                <div class="portlet-body form">
                    <!-- BEGIN FORM-->
                    <div class="form-horizontal form-view">
                        @*<form action="#" class="form-horizontal">*@
                        <h3>View Owner Info  - @Model.UserProfile.First_Name @Model.UserProfile.Last_Name </h3>
                        <h3 class="form-section">Owner Personal Info </h3>
                        <div class="row-fluid">
                            <div class="span6 ">
                                <div class="control-group">
                                    <label class="control-label">User Name:</label>
                                    <div class="controls">
                                        <span class="text">@Model.User.UserName</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <div class="span6 ">
                                <div class="control-group">
                                    <label class="control-label">Email:</label>
                                    <div class="controls">
                                        <span class="text">@Model.User.email</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <div class="row-fluid">
                            <div class="span6 ">
                                <div class="control-group">
                                    <label class="control-label">First Name:</label>
                                    <div class="controls">
                                        <span class="text">@Model.UserProfile.First_Name</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <div class="span6 ">
                                <div class="control-group ">
                                    <label class="control-label">Last Name:</label>
                                    <div class="controls">
                                        <span class="text">@Model.UserProfile.Last_Name</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                        </div>

                        <!--/row-->
                        <div class="row-fluid">
                            <div class="span6 ">
                                <div class="control-group">
                                    <label class="control-label">Gender:</label>
                                    <div class="controls">
                                        <span class="text">@Model.UserProfile.Gender</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <div class="span6 ">
                                <div class="control-group">
                                    <label class="control-label">Birthday Date:</label>
                                    <div class="controls">
                                        <span class="text">@Model.UserProfile.B_Date</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <!--/row-->
                        <div class="row-fluid">
                            <div class="span6 ">
                                <div class="control-group">
                                    <label class="control-label">Country:</label>
                                    <div class="controls">
                                        <span class="text">@Country</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <div class="span6 ">
                                <div class="control-group">
                                    <label class="control-label">City:</label>
                                    <div class="controls">
                                        <span class="text">@Model.UserProfile.City</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <!--/row-->
                        <!--/row-->
                        <div class="row-fluid">
                            <div class="span6 ">
                                <div class="control-group">
                                    <label class="control-label">Religion</label>
                                    <div class="controls">
                                        <span class="text">@Religion</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <div class="span6 ">
                                <div class="control-group">
                                    <label class="control-label">Zipcode</label>
                                    <div class="controls">
                                        <span class="text">@Model.UserProfile.Zipcode</span>
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <!--/row-->
                        <h3 class="form-section">Company Information</h3>

                        <div class="row-fluid">
                            <div class="span12">
                                <!-- BEGIN PORTLET-->
                                <div class="portlet box blue">

                                    <div class="portlet-body form">
                                        <!-- BEGIN FORM-->
                                        @* <form action="#" class="form-horizontal"> *@
                                        <div class="control-group">
                                            <label class="control-label">Company Logo Upload:</label>
                                            <div class="controls">
                                                <span class="text">@Model.Company.CompanyLogo</span>
                                            </div>
                                        </div>
                                        @*</form>*@
                                        <!-- END FORM-->
                                    </div>
                                </div>
                                <!-- END PORTLET-->
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span12 ">
                                <div class="control-group">
                                    <label class="control-label">Comapny Name:</label>
                                    <div class="controls">
                                        <span class="text">@Model.Company.CompanyName</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--/row-->
                        <div class="form-actions">
                            <span class="btn blue">
                                <i class="icon-pencil"></i>@Html.ActionLink("Edit", "Edit", new { id = Model.User.ID }, new { @class = "btn blue" })
                            </span>
                            @Html.ActionLink("Back", "Index", "OwnerCreate", new { @class = "btn" })
                        </div>
                    </div>
                    <!-- END FORM-->
                </div>
                <!-- end form -->
            </div>
        </div>
        <!-- END PAGE -->
    </div>
    <!-- END CONTAINER -->
    </div>
    @Html.Partial("_footer")
    <script>
        jQuery(document).ready(function () {
            // initiate layout and plugins
            App.init();
        });
    </script>
    <script>
        jQuery(document).ready(function () {
            // initiate layout and plugins
            App.setPage("table_editable");
            App.init();
        });
    </script>
    <script>
        jQuery(document).ready(function () {

            // to fix chosen dropdown width in inactive hidden tab content
            $('.advance_form_with_chosen_element').on('shown', function (e) {
                App.initChosenSelect('.chosen_category:visible');
            });

            // initiate layout and plugins
            App.init();
        });
    </script>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}


